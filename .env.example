# Core Agent Environment Variables
# Copy this file to .env and update with your actual values

# API Keys
AZURE_OPENAI_ENDPOINT=
OPENAI_API_KEY=
OPENAI_API_VERSION=
AZURE_DEPLOYMENT_NAME=
LANGCHAIN_API_KEY=
LANGCHAIN_TRACING_V2=false

# Database URLs (automatically set by docker-compose)
REDIS_URL=redis://:redis_password@localhost:6379/0
POSTGRES_URL=postgresql://core_agent_user:postgres_password@localhost:5432/core_agent_db
MONGODB_URL=mongodb://admin:mongo_password@localhost:27017/core_agent_db?authSource=admin

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8000

# Python Settings
PYTHONPATH=.
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1